{
  "name": "catstronauts-server-complete",
  "version": "1.0.0",
  "description": "back-end demo app for Apollo's lift-off course",
  "main": "src/index.ts",
  "scripts": {
    "start": "node src/index.js",
    "build": "tsc",
    "dev": "nodemon src/index.ts",
    "postinstall": "tsc",
    "watch-node": "nodemon src/index.ts",
    "watch-ts": "tsc -w"
  },
  "dependencies": {
    "apollo-datasource-rest": "^3.3.0",
    "apollo-server": "^3.4.0",
    "graphql": "^15.6.1",
    "typescript": "^4.4.4"
  },
  "devDependencies": {
    "@types/node": "^16.10.5",
    "dotenv": "^8.2.0",
    "nodemon": "^2.0.4",
    "ts-node": "^10.3.0"
  },
  "engines": { "node": "14.x" },
  "nodemonConfig": {
    "ext": "ts",
    "exec": "npm run build && npm start"
  },
  "author": "Raphael Terrier @R4ph-t",
  "license": "MIT",
  "private": true
}
